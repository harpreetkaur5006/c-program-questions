#include <stdio.h>
int binaryToDecimal(char binary[]) {
    int decimal = 0;
    int i = 0;
    // Find the length of the binary string
    while (binary[i] != '\0') {
        i++;
    }
    int power = 0;
    // Traverse from right to left
    for (int j = i - 1; j >= 0; j--) {
        if (binary[j] == '1') {
            decimal += (1 << power);  // 2^power
        }
        power++;
    }
    return decimal;
}
int main(){
    int T;
    char no[32];
    scanf("%d",&T);
    while(T--){
        scanf("%s",no);
        printf("%d\n",binaryToDecimal(no));
    }
     return 0;
}

