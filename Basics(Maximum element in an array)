#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include <limits.h>
int maxElement(int arr[], int N) {
  // Write Your Code here
  int big=arr[0];
    for(int i=0;i<N;i++){
        if(arr[i]>big){
            big=arr[i];
        }
    }
    return big;
}
int main(){
  int i, size;
  scanf("%d", &size);
  int array[size]; 
  for(i = 0; i < size; i++)
    scanf("%d", &array[i]);
  printf("%d", maxElement(array, size));
  return 0;
}
