#include <stdio.h>
/*
 * Complete the function 'gcd'
 * @params
 *   i -> first integer
 *   j -> second integer
 *
 *  @returns
 *  an integer, denoting the gcd of i and j
 */
int gcd(int i, int j){ 
    while (j != 0) {
        int temp = j;
        j = i % j;
        i = temp;
    }
    return i;
}
int main(){
  int i, j;
  scanf("%d",&i);
  scanf("%d",&j);
  printf("%d\n",gcd(i, j));
  return 0;
}
