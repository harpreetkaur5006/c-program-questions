#include<iostream>
#include<cstdio>
#include<cmath>
// Include headers as needed
using namespace std;
int main()
{
    int testcase;
    cin >> testcase;// no. of testcase  inshort n
    for(int k=0;k<testcase;k++){
   int n;
    cin >> n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin >> arr[i];
    }
    int index;
    cin >> index;
    int R=index%n;// here R is new index 
   bool first =true;
            for(int i=R; i<n;i++){
                if(!first) cout << " ";
                cout << arr[i];
                first =false;
            }
        for(int j=0;j<R;j++){
            if(!first) cout << " ";
            cout << arr[j] ;
            first =false;
        }
        cout <<endl;
    }
    return 0;
}
